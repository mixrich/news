.not-ready {


  &__cta {
    text-align: center;
  }

  &__list {
    list-style: none;
    padding: 0;

    &__item {
      display: inline-block;
      vertical-align: top;
      box-sizing: border-box;
      padding: 10px;
      width: 25%;



    }

    &__link {
        background-color: rgb(250,250,250);
        display: inline-block;
        padding: 10px;
        borderRadius(5px);
        box-shadow: 0 0 2px #000;
        borderBox();
        width: 100%;
        &:hover {
          box-shadow: 0 0 8px black;
        }

        & span {
          font-weight: 600;
          display: block;
          margin-bottom: 10px;
        }

    }
  }



}
